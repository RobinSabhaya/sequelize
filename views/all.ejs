<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>

  <body>
    <table border="2px solid black">
      <tr>
        <th>id</th>
        <th>name</th>
        <th>address</th>
        <th>mobile</th>
      </tr>
      <% data.forEach(ele=>{%>
        <tr>
        <td><%= ele.dataValues.id %></td>
        <td><%= ele.dataValues.name %></td>
        <td><%= ele.dataValues.address %></td>
        <td><%= ele.dataValues.mobile %></td>
        <td><%= ele.dataValues.orderTable.date %></td>
        <td><%= ele.dataValues.orderTable.totalAmount %></td>
        <td>
          <button><a href="/updatecustomer/<%= ele.dataValues.id %>">edit</a></button>
        </td>
        <td>
          <button><a href="/deletecustomer/<%= ele.dataValues.id %>">delete</a></button>
        </td>
      </tr>
        <%})%>
       
    </table>
  </body>
</html>
